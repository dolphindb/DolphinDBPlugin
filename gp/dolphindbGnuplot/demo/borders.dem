unset grid
set xrange [0:10]
set yrange [0:10]
set format ""
#
set multiplot layout 4, 4 title "Demonstration of different border settings"
do for [bb = 0:15] {
    set border bb
    set label 1 sprintf("Border = %.0f",bb)  at 5,5 center
    plot 1/0 notitle
}
unset multiplot

pause -1 "Hit return to continue"
reset
